{
  "name": "@whoj/nuxt3-axios",
  "version": "1.0.0",
  "description": "An alternative to @nuxtjs/axios for Nuxt 3",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepublishOnly": "run-s clear lint build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "clear": "rm -rf dist",
    "cleanup": "nuxi cleanup playground",
    "prepare": "run-s cleanup dev:prepare",
    "lint": "eslint \"{**/*,*}.{ts,tsx,vue}\" --fix",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.8",
    "@nuxt/schema": "^3.0.0-rc.8",
    "@vueuse/integrations": "^9.5.0",
    "@whoj/utils-core": "^1.1.1",
    "axios": "^1.1.3",
    "axios-retry": "^3.3.1",
    "defu": "^6.1.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.0",
    "@nuxt/schema": "3.0.0-rc.13",
    "@nuxt/test-utils": "3.0.0-rc.13",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@vueuse/core": "^9.5.0",
    "@whoj/eslint-config": "^1.0.2",
    "bumpp": "^8.2.1",
    "core-js": "^3.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.0.0-rc.12",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.2",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "@vueuse/core": "^9.3.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.15.11"
    }
  },
  "packageManager": "pnpm@7.16.0",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/who-jonson/nuxt3-axios/issues"
  },
  "homepage": "https://github.com/who-jonson/nuxt3-axios#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/who-jonson/nuxt3-axios.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
