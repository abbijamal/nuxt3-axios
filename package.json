{
  "name": "@whoj/nuxt3-axios",
  "version": "0.1.0",
  "description": "An alternative to @nuxtjs/axios for Nuxt 3",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepublishOnly": "run-s clear lint build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "clear": "rm -rf dist",
    "lint": "eslint \"{**/*,*}.{ts,tsx,vue}\" --fix",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.6",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "^3.0.0-rc.6",
    "@nuxtjs/eslint-config-typescript": "latest",
    "bumpp": "^8.2.1",
    "core-js": "^3.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.0.0-rc.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.19.1",
    "vue": "^3.2.37",
    "webpack": "^5.74.0"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/who-jonson/nuxt3-axios/issues"
  },
  "homepage": "https://github.com/who-jonson/nuxt3-axios#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/who-jonson/nuxt3-axios.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
