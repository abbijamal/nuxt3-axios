{
  "name": "@whoj/nuxt3-axios",
  "version": "1.2.0",
  "description": "An alternative to @nuxtjs/axios for Nuxt 3",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepublishOnly": "run-s clear lint build",
    "release": "bumpp --tag --commit --push && pnpm publish",
    "test": "vitest",
    "clear": "rm -rf dist",
    "cleanup": "nuxi cleanup playground",
    "prepare": "run-s cleanup dev:prepare",
    "lint": "eslint \"{**/*,*}.{ts,tsx,vue}\" --fix",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@vueuse/integrations": "^10.0.2",
    "@whoj/utils-core": "^1.7.3",
    "axios": "^1.3.5",
    "axios-retry": "^3.4.0",
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/test-utils": "3.4.1",
    "@types/node": "^18.15.11",
    "@vueuse/core": "^10.0.2",
    "@whoj/eslint-config": "^1.4.0",
    "bumpp": "^9.1.0",
    "core-js": "^3.30.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "@nuxt/schema": "^3.0.0",
    "@vueuse/core": "^9 || ^10"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.17.17",
      "vue-demi": "^0.14.0",
      "typescript": "$typescript",
      "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@latest"
    }
  },
  "packageManager": "pnpm@8.2.0",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/who-jonson/nuxt3-axios/issues"
  },
  "homepage": "https://github.com/who-jonson/nuxt3-axios#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/who-jonson/nuxt3-axios.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}